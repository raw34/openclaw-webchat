{
  "name": "openclaw-webchat",
  "version": "0.1.0",
  "description": "Core WebSocket client for OpenClaw Gateway",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "webchat",
    "ai",
    "chat",
    "websocket"
  ],
  "author": "Randy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raw34/openclaw-webchat.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/raw34/openclaw-webchat/issues"
  },
  "homepage": "https://github.com/raw34/openclaw-webchat/tree/main/packages/core#readme",
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
