{
  "name": "@openclaw/chat-core",
  "version": "0.1.0",
  "description": "Core WebSocket client for OpenClaw Gateway",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "webchat",
    "ai",
    "chat",
    "websocket"
  ],
  "author": "Randy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/openclaw-webchat"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
