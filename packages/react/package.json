{
  "name": "openclaw-webchat-react",
  "version": "0.1.0",
  "description": "React components and hooks for OpenClaw WebChat",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "webchat",
    "react",
    "chat",
    "ai"
  ],
  "author": "Randy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raw34/openclaw-webchat.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/raw34/openclaw-webchat/issues"
  },
  "homepage": "https://github.com/raw34/openclaw-webchat/tree/main/packages/react#readme",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "openclaw-webchat": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
