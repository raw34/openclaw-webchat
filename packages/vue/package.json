{
  "name": "@raw34/openclaw-webchat-vue",
  "version": "0.1.0",
  "description": "Vue components and composables for OpenClaw WebChat",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css",
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && cp src/index.d.ts dist/",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "webchat",
    "vue",
    "chat",
    "ai"
  ],
  "author": "Randy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raw34/openclaw-webchat.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/raw34/openclaw-webchat/issues"
  },
  "homepage": "https://github.com/raw34/openclaw-webchat/tree/main/packages/vue#readme",
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@raw34/openclaw-webchat": "workspace:*"
  },
  "devDependencies": {
    "esbuild-plugin-vue3": "^0.5.1",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "unplugin-vue-components": "^31.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^3.2.4"
  }
}
