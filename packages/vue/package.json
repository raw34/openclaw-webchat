{
  "name": "@openclaw/chat-vue",
  "version": "0.1.0",
  "description": "Vue components and composables for OpenClaw WebChat",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openclaw",
    "webchat",
    "vue",
    "chat",
    "ai"
  ],
  "author": "Randy",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@openclaw/chat-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vue": "^3.5.0"
  }
}
